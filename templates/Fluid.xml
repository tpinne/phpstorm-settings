  <template name="fluid_widget_paginate" value="&lt;f:widget.paginate objects=&quot;{$OBJECTS$}&quot; as=&quot;$POBJECTS$&quot; configuration=&quot;{itemsPerPage: 5, insertAbove: 1, insertBelow: 0, maximumNumberOfLinks: 10}&quot;&gt;&#10;&#9;&lt;f:for each=&quot;{$POBJECTS$}&quot; as=&quot;object&quot;&gt;&#10;&#9;&#10;&#9;&lt;/f:for&gt;&#10;&lt;/f:widget.paginate&gt;" description="Widget: Paginate" toReformat="true" toShortenFQNames="true">
