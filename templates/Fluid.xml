  <template name="fluid_form_validationResults" value="&lt;f:form.validationResults&gt;&#10;&#9;&lt;f:if condition=&quot;{$VALIDATIONRESULTS$.flattenedErrors}&quot;&gt;&#10; &#9;&#9;&lt;ul class=&quot;errors&quot;&gt;&#10; &#9;&#9;&#9;&lt;f:for each=&quot;{$VALIDATIONRESULTS$.flattenedErrors}&quot; as=&quot;errors&quot; key=&quot;propertyPath&quot;&gt;&#10; &#9;&#9;&#9;&lt;li&gt;{propertyPath}&#10; &#9;&#9;&#9;&#9;&lt;ul&gt;&#10; &#9;&#9;&#9;&#9;&#9;&lt;f:for each=&quot;{errors}&quot; as=&quot;error&quot;&gt;&#10; &#9;&#9;&#9;&#9;&#9;&lt;li&gt;{error.code}: {error}&lt;/li&gt;&#10; &#9;&#9;&#9;&#9;&#9;&lt;/f:for&gt;&#10; &#9;&#9;&#9;&#9;&lt;/ul&gt;&#10; &#9;&#9;&#9;&lt;/li&gt;&#10;&#9; &#9;&#9;&lt;/f:for&gt;&#10; &#9;&#9;&lt;/ul&gt;&#10;&#9;&lt;/f:if&gt;&#10; &lt;/f:form.validationResults&gt;" description="Form validation results" toReformat="true" toShortenFQNames="true">
